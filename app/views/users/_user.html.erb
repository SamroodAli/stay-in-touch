<li class="user">
  <%= link_to user.name, user %>
  <span class="profile-link">
    <h3><%= link_to 'See Profile',  user_path(user), class: 'profile-link' %></h3>
    <% unless current_user.friend?(user) %>
      <%= render 'friendship_button', user: user ,index:index%>
    <% end %>
  </span>
</li>
